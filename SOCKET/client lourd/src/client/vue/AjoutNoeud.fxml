<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.vue.AjoutNoeudController">
  <children>
    <StackPane>
      <children>
        <VBox spacing="10.0">
          <children>
            <GridPane>
              <children>
                <Label text="TitreNoeud" GridPane.columnIndex="0" />
                <Label text="Longueur" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                <TextField fx:id="titre" promptText="Entrez le nom du noeud" GridPane.columnIndex="1" />
                <TextField fx:id="longueur" promptText="entrez la longueur"  GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="Largeur" GridPane.rowIndex="3" />
                        <Label text="NoeudPere" GridPane.rowIndex="4" />
                        <TextField fx:id="largeur" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <TextField fx:id="nomPere" promptText="Entrez le nom du noeud pere" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <Label text="Description" GridPane.rowIndex="1" />
                        <TextField fx:id="description" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                      <Button mnemonicParsing="false" onAction="#ajout" text="Ajouter" />
                    </buttons>
                  </ButtonBar>
          </children>
        </VBox>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
